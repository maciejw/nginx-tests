{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5080"
      },
      "Https": {
        "Url": "https://localhost:5443",
        "ClientCertificateMode": "AllowCertificate",
        "Certificate": {
          "Path": "../certs/localhost.crt",
          "KeyPath": "../certs/localhost.key"
        }
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "Filter": {
      "Microsoft": "Verbose",
      "System": "Vebose",
      "*": "Verbose"
    }
  }
}